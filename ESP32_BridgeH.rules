
(rules PCB ESP32_BridgeH
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 203)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.7)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_1000:500_um"
    (shape
      (circle F.Cu 1000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_1000:500_um" "Via[0-1]_1000:500_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_1000:500_um-kicad_default" "Via[0-1]_1000:500_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-power" "Via[0-1]_800:400_um" power
  )
  (via 
    "Via[0-1]_1000:500_um-power" "Via[0-1]_1000:500_um" power
  )
  (via 
    "Via[0-1]_800:400_um-signal" "Via[0-1]_800:400_um" signal
  )
  (via 
    "Via[0-1]_1000:500_um-signal" "Via[0-1]_1000:500_um" signal
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    power "Via[0-1]_1000:500_um-power"
  )
  (via_rule
    signal "Via[0-1]_1000:500_um-signal"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    /ADC
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class power
    GND /Vin
    (clearance_class power)
    (via_rule power)
    (rule
      (width 889.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class signal
    /reset /Vcc "Net-(D1-Pad1)" /A02 /A01 /B02 /B01 "Net-(J3-Pad1)"
    /RX /TX "Net-(J4-Pad5)" /PWA /Ain1 /Ain2 /Bin1 /Bin2
    /PWB "Net-(U2-Pad37)" /SCL /SDA "Net-(U2-Pad32)" "Net-(U2-Pad31)" "Net-(U2-Pad30)" "Net-(U2-Pad29)"
    "Net-(U2-Pad28)" "Net-(U2-Pad27)" /INT "Net-(U2-Pad24)" "Net-(U2-Pad23)" "Net-(U2-Pad22)" "Net-(U2-Pad21)" "Net-(U2-Pad20)"
    "Net-(U2-Pad19)" "Net-(U2-Pad18)" "Net-(U2-Pad17)" "Net-(U2-Pad10)" "Net-(U2-Pad7)" "Net-(U2-Pad6)" "Net-(U2-Pad5)" "Net-(U2-Pad4)"
    "Net-(U3-Pad5)" "Net-(U3-Pad6)" "Net-(U3-Pad7)"
    (clearance_class signal)
    (via_rule signal)
    (rule
      (width 762.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)